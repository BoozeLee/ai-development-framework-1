# AI Development Framework - Environment Variables Template
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_ACCOUNT_ID=your_aws_account_id_here
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1
AWS_S3_OUTPUT_BUCKET=s3://your-athena-results-bucket/

# AIOS Configuration
AIOS_API_KEY=your_aios_api_key_here
AIOS_SECRET_KEY=your_aios_secret_key_here

# AI Service API Keys
CREWAI_OPENAI_API_KEY=your_openai_api_key_here
CREWAI_ANTHROPIC_API_KEY=your_anthropic_api_key_here
CREWAI_GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration
AIOS_DATABASE_URL=postgresql://user:pass@localhost:5432/aios_prod
AIOS_REDIS_URL=redis://localhost:6379/0

# Application Configuration
AIOS_ENVIRONMENT=production
AIOS_LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_here
